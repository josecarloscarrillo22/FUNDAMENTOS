#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
float A,B,aux;
cout<<"inserte A:"<<endl;
cin>>A;
cout<<"inserte B:"<<endl;
cin>>B;
aux=A;
A=B;
B=aux;
cout<<"el nuevo valor de A es: "<<A<<endl;
cout<<"el nuevo valor de B es: "<<B<<endl;
getch();
}
PSEINT
Proceso cambiodevalores
Escribir "inserte a"
Leer a
Escribir "inserte b"
Leer b
aux<-a
a<-b
b<-aux
Escribir "el valor de a es:",a
Escribir "el valor de b es:",b
FinProceso 
